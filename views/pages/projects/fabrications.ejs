                                          <div class="project h-100">
                                            
                                             <div class="project_img h-100">
                                                <figure class="h-100"><img class="h-100" src="/images/upload_fabrication_images/<%=data[i][j].image%>" alt="#"/></figure>
                                             </div>
                                             <div id="pro_ho" class="project_text w-100">
                                                <h3> <%- data[i][j].title; %></h3> `
                                                <p> <%- data[i][j].description;%></p>
                                             </div>
                                          </div>
                                 
                                       </div>
                                       <% } %>
                  
                                       </div>
                                    </div>
                                 </div>
                              </div>
